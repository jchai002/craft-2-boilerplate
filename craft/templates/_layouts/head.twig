<head>
	<meta charset="utf-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
	<title>{% if title is defined %}{{ title }} - {% endif %}{{ siteName }}</title>
	<link rel="home" href="{{ siteUrl }}" />
  {% if craft.config.environmentVariables.blockCrawlers is defined and craft.config.environmentVariables.blockCrawlers == 1 %}
      <META NAME="ROBOTS" CONTENT="NOINDEX, NOFOLLOW">
  {% endif %}
  <!-- custom css -->
  <link rel="stylesheet" type="text/css" href="{{ siteUrl }}dist/bundle.min.css">
</head>
